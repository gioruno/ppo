        <div class="main">
            <table style="width: 95%;" align="center">
                <tr>
                    <td class="style2">
                        <asp:PlaceHolder ID="PlaceHolder1" runat="server"></asp:PlaceHolder>
                    </td>
                    <td class="style3">
                        <asp:ContentPlaceHolder ID="MainContent" runat="server">
                            
                        </asp:ContentPlaceHolder>              
                    </td>
                </tr>
                
            </table>
            <br />
            
