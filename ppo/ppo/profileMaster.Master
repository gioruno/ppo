                <asp:HyperLink ID="linkUser" runat="server" NavigateUrl="~/profile.aspx">[linkUser]</asp:HyperLink>
&nbsp; </br>
                <asp:HyperLink ID="logLink" runat="server">Login</asp:HyperLink>
                &nbsp;&nbsp;&nbsp;&nbsp;
